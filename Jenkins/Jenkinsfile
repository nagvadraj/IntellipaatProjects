pipeline {
    agent any
    
    stages {
        stage('Pull Git Content') {
            steps {
                script {
                    if (!fileExists('develop')) {
                        mkdir('develop')
                    }
                }                
                dir('createdbyJenkinsPipeline') {
                    git url: 'https://github.com/nagvadraj/IntellipaatProjects.git', branch: 'develop'
                }
            }
        }
    }
}
